<portlet-app version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>upgrade-calculator</portlet-name>
		<display-name>upgrade-calculator</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/upgrade-calculator.xml</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/WEB-INF/jsp/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>upgrade-calculator</title>
			<short-title>upgrade-calculator</short-title>
			<keywords>upgrade-calculator</keywords>
		</portlet-info>
	</portlet>
</portlet-app>